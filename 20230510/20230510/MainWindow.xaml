<Window x:Class="_20230510.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_20230510"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="600">
  <Grid>
    <Grid.ColumnDefinitions>
      <ColumnDefinition/>
      <ColumnDefinition Width="150"/>
    </Grid.ColumnDefinitions>
    <Canvas>
      <local:GeoLine x:Name="MyGeoLine1" Anchors="0,0 200,20 100,100" Stroke="Red"
                     StrokeThickness="10"/>
    </Canvas>
    <StackPanel Grid.Column="1">
      <Button Content="test1" Click="Button_Click"/>
      <Button Content="test2" Click="Button_Click_2"/>
      <Button Content="test3" Click="Button_Click_3"/>
      <Button Content="test4" Click="Button_Click_4"/>
      <Button Content="check" Click="Button_Click_1"/>
    </StackPanel>
  </Grid>
</Window>
